{
  "kind": "collectionType",
  "collectionName": "series_contents",
  "info": {
    "singularName": "series-content",
    "pluralName": "series-contents",
    "displayName": "SeriesContent"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "description": {
      "type": "richtext"
    },
    "youtubeLink": {
      "type": "dynamiczone",
      "components": [
        "blocks.youtube-link"
      ]
    },
    "Author": {
      "type": "string"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "seriesName": {
      "type": "enumeration",
      "enum": [
        "Intro To Jewish Philosophy",
        "Philosophy Terms"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "Title"
    }
  }
}
