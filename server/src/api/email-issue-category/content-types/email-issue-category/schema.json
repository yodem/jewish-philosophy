    "prefix": {
      "type": "string",
      "required": true,
      "maxLength": 20,
      "minLength": 2,
      "unique": true
    },
    "description": {
      "type": "text",
      "maxLength": 500
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  }
}
